public class matrixaddition {
  
    public static void main(String[] args){

        int a[][]={{1,2},{4,5}};
        int b[][]={{1,2},{4,5}};
        int c[][]=new int[2][2];
        for(int i=1;i<2;i++){
            for(int j=1;j<2;j++){
                c[i][j]=a[i][j]+b[i][j];
                System.out.println(c[i][j]+" ");
            }
            System.out.println();
        }

    }
    
}
